# Contoh .env untuk lingkungan Docker (lokal)
# Set DOCKER=1 agar AppServiceProvider mendeteksi lingkungan Docker
DOCKER=1
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost

# Queue/cache/session biasanya mengarah ke service terpisah di docker-compose
QUEUE_DRIVER=redis
CACHE_DRIVER=redis
SESSION_DRIVER=redis
FILESYSTEM_DRIVER=local

# Database (contoh nama service di docker-compose)
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=presensiku
DB_USERNAME=root
DB_PASSWORD=secret

# Redis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379
