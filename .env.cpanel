# Contoh .env untuk cPanel / shared hosting (tidak menggunakan Docker)
# Jangan set DOCKER=1 di cPanel; biarkan 0 atau kosong
DOCKER=0
APP_ENV=production
APP_DEBUG=false
APP_URL=https://your-production-domain.example

# Gunakan driver lokal/inline untuk menghindari dependency eksternal
QUEUE_DRIVER=sync
CACHE_DRIVER=file
SESSION_DRIVER=file
FILESYSTEM_DRIVER=local

# Database: sesuaikan dengan kredensial hosting cPanel Anda
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=presensiku_prod
DB_USERNAME=cpanel_user
DB_PASSWORD=your_db_password

# Redis tidak dipakai di cPanel contoh ini; hapus atau kosongkan jika tidak tersedia
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
